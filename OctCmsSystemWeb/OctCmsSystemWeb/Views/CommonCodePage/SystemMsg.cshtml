
@{
    Layout = null;
}

<!DOCTYPE html>

<html style="overflow: auto; overflow-x: hidden;">
<head>
    <!--
           <meta name="viewport" content="width=device-width initial-scale="1" />
       viewport是网页默认的宽度和高度，上面这行代码的意思是，网页宽度默认等于屏幕宽度（width=device-width），
       原始缩放比例（initial-scale=1）为1.0，即网页初始大小占屏幕面积的100%。 user-scalable=no  不允许缩放网页 -->
    <meta name="viewport" content="width=800,user-scalable=no" initial-scale="1" />
    <title>系统异常信息(系统级异常错误)</title>
    <link href="../Assets/Lib/AmazeUI-2.7.2/css/amazeui.min.css" rel="stylesheet" />
    <link href="../Assets/Lib/AdminFrame/AdminPagePublic.css" rel="stylesheet" />

    <script src="../Assets/Lib/AmazeUI-2.7.2/js/jquery-2.2.2.min.js"></script>
    <script src="../Assets/Lib/AmazeUI-2.7.2/js/amazeui.min.js"></script>
    <script src="../Assets/Lib/AdminFrame/AdminPagePublic.js"></script>
    <script src="../Assets/Lib/AlertConfirmWin/AlertConfirmWin.js"></script>
    <script src="../Assets/Js/CommonCodePage/SystemMsg.js"></script>

</head>
<body>


    <!--面包屑导航-->
    <div class="nav am-cf am-nbfc" data-am-sticky="{top:0}">
        <ul class="am-breadcrumb">
            <li><a href="../NavPage/Index" class="am-icon-home">首页</a></li>
            <li><a href="../NavPage/NavSubList?NavName=系统管理">系统管理</a></li>
            <li class="am-active">系统异常信息</li>
        </ul>
    </div>

    <!--主体内容-->
    <div class="main am-nbfc">

        <!--标题栏-->
        <div class="main-title am-cf am-nbfc">
            <div class="title-txt-div am-fl">
                <i class="am-icon-list"></i> 系统异常信息
            </div>
            <div class="title-btn-div am-fl">
                @*<button class="title-btn am-btn am-btn-xs am-btn-danger am-round am-margin-left-xs" onclick="window.location.href='../UserGoodsShopPage/ShopAdd'"><i class="am-icon-plus-circle"></i> 添加店铺</button>*@
            </div>
        </div>

        <!--搜索栏-->
        <div class="search-bar am-btn-toolbar am-cf">

            <div class="am-btn-group am-btn-group-xs am-cf item-txt-css">
                <input type="number" class="keyword-input am-form-field am-fl" id="SysMsgID_se" placeholder="信息ID" />
            </div>
            <div class="am-btn-group am-btn-group-xs am-cf item-txt-css">
                <input type="text" class="keyword-input am-form-field am-fl" id="OrderIDArr_se" placeholder="涉及订单ID" />
            </div>
            <div class="am-btn-group am-btn-group-xs am-cf item-txt-css">
                <input type="text" class="keyword-input am-form-field am-fl" id="UserIDArr_se" placeholder="涉及用户ID" />
            </div>
            <div class="am-btn-group am-btn-group-xs am-cf item-txt-css">
                <input type="text" class="keyword-input am-form-field am-fl" id="MsgTitle_se" placeholder="信息标题" />
            </div>
            <div class="am-btn-group am-btn-group-xs am-cf item-txt-css">
                <input type="text" class="keyword-input am-form-field am-fl" id="MsgContent_se" placeholder="信息内容 " />
            </div>
            <div class="am-btn-group am-btn-group-xs am-cf" style="width: 150px;">
                <select id="MsgType_se" data-am-selected="{btnWidth:'130',btnSize: 'xs',btnStyle: 'default'}">
                    <option value=" " selected>信息类别</option>
                    <option value="OnLinePay">在线支付</option>
                    <option value="BalancePay">余额支付</option>
                    <option value="ShopInEx">商家收支</option>
                    <option value="BuyerInEx">买家收支</option>
                    <option value="ShopIntegralRecharge">商家充积分</option>
                    <option value="GroupRefund">拼团订单退款</option>
                    <option value="GroupOrder">拼团订单</option>
                    <option value="Commission">订单抽成与分红分润</option>
                    <option value="InExDividend">分润收支</option>
                    <option value="IntegralDividend">分润积分</option>
                    <option value="BuyerWithDraw">买家余额提现</option>
                    <option value="AggregateOrderPay">扫码聚合支付</option>
                </select>
            </div>
            <div class="am-btn-group am-btn-group-xs am-cf" style="width: 150px;">
                <select id="SendToType_se" data-am-selected="{btnWidth:'130',btnSize: 'xs',btnStyle: 'default'}">
                    <option value=" " selected>发送对象</option>
                    <option value="System">系统本身</option>
                    <option value="Shop">商家</option>
                    <option value="Buyer">买家</option>
                </select>
            </div>

            <div class="am-btn-group am-btn-group-xs am-cf item-txt-css">
                <input type="text" class="keyword-input am-form-field am-fl" id="ExtraData_se" placeholder="额外数据(如:ID号)" />
            </div>

            <div class="am-btn-group am-btn-group-xs am-cf" style="width: 150px;">
                <select id="IsRead_se" data-am-selected="{btnWidth:'130',btnSize: 'xs',btnStyle: 'default'}">
                    <option value=" " selected>已读</option>
                    <option value="false">否</option>
                    <option value="true">是</option>
                </select>
            </div>

            <div class="datetime-div am-btn-group am-btn-group-xs am-cf" style="width: 150px;">
                <div class="am-form-group am-form-icon">
                    <i class="am-icon-calendar"></i>
                    <input type="text" id="WriteDate_se" style="background:none; width: 130px;" class="datetime-input am-form-field" placeholder="写入时间" data-am-datepicker="{theme: 'success',}" readonly>
                </div>
            </div>

            <div class="am-btn-group am-btn-group-xs am-cf" style="width: 210px;">
                <button type="button" id="btnSearch" class="search-btn am-fl am-btn am-radius am-btn-success">搜索</button>
            </div>


        </div>

        <!--表单区域-->
        <div class="table-div">

            <table class="table-content am-table-striped am-table-hover" width="100%">
                <thead>
                    <tr class="table-head am-success">
                        <td width="7%">信息ID</td>
                        <td width="10%">信息类别 </td>
                        <td width="8%">涉及订单ID</td>
                        <td width="8%">涉及用户ID</td>
                        <td width="6%">发送对象</td>
                        <td width="11%">信息标题</td>
                        <td width="26%">信息内容</td>
                        <td width="8%">额外数据</td>
                        <td width="6%">已读</td>
                        <td width="10%">写入时间</td>
                    </tr>
                </thead>
                <tbody id="TbodyTrPage">
                    <tr>
                        <td>234324</td>
                        <td>OnLinePay<br />(在线支付)</td>
                        <td><a href="#" target="_blank">662020110615495163515538080</a></td>
                        <td><a href="#" target="_blank">10006</a></td>
                        <td>System</td>
                        <td>分润余额转入账户余额收支异常</td>
                        <td>买家分润余额收支出现异常，买家UserID:10006，请及时核对信息！两种方式分润余额统计不一致！SumCurrentBalance=156.84050，RecentCurrentBalance=146.29850</td>
                        <td>662020110615495163515538080</td>
                        <td>
                            是
                        </td>
                        <td>
                            2019-05-05 13:27:45
                        </td>
                    </tr>
                </tbody>
            </table>

            <!--表格信息操作区与分页-->
            <div class="table-exe-div am-cf">
                <div class="table-exe-btn am-fl">
                    <div class="table-btn-bottom am-btn-group am-btn-group-xs">
                        <button type="button" class="am-btn am-btn-default" onclick="delSystemMsgMonthAgo('3')"><span class="am-icon-trash-o"></span>删除三个月前的信息</button>
                        <button type="button" class="am-btn am-btn-default" onclick="delSystemMsgMonthAgo('6')"><span class="am-icon-trash-o"></span>删除半年前的信息</button>
                        <button type="button" class="am-btn am-btn-default" onclick="delSystemMsgMonthAgo('12')"><span class="am-icon-trash-o"></span>删除一年前的信息</button>
                    </div>
                </div>
                <div class="page-bar am-fr">
                    <ul class="am-pagination" id="PageBar1">
                        <li><a href="#">«</a></li>
                        <li><a href="#">1</a></li>
                        <li><span>...</span></li>
                        <li><a href="#">2</a></li>
                        <li><a href="#">3</a></li>
                        <li class="am-active"><a href="#">4</a></li>
                        <li><a href="#">5</a></li>
                        <li><a href="#">6</a></li>
                        <li><span>...</span></li>
                        <li><a href="#">500</a></li>
                        <li><input type="text" class="page-go-text am-form-field" placeholder="跳转页" /></li>
                        <li><a href="#">»</a></li>
                    </ul>
                </div>
            </div>

            <!--备注说明-->
            <div class="memo-content">
                系统异常信息(系统级异常错误)
            </div>


        </div>
    </div>


</body>
</html>
