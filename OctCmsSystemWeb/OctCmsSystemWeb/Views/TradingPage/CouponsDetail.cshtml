
@{
    Layout = null;
}

<!DOCTYPE html>

<html style="overflow: auto; overflow-x: hidden;">
<head>
    <!--
         <meta name="viewport" content="width=device-width initial-scale="1" />
     viewport是网页默认的宽度和高度，上面这行代码的意思是，网页宽度默认等于屏幕宽度（width=device-width），
     原始缩放比例（initial-scale=1）为1.0，即网页初始大小占屏幕面积的100%。 user-scalable=no  不允许缩放网页 -->
    <meta name="viewport" content="width=800,user-scalable=no" initial-scale="1" />
    <title>优惠券详情</title>
    <link href="../Assets/Lib/AmazeUI-2.7.2/css/amazeui.min.css" rel="stylesheet" />
    <link href="../Assets/Lib/AdminFrame/AdminPagePublic.css" rel="stylesheet" />
    <link href="../Assets/Css/TradingPage/OrderDetail.css" rel="stylesheet" />

    <script src="../Assets/Lib/AmazeUI-2.7.2/js/jquery-2.2.2.min.js"></script>
    <script src="../Assets/Lib/AdminFrame/AdminPagePublic.js"></script>
    <script src="../Assets/Lib/AlertConfirmWin/AlertConfirmWin.js"></script>
    <script src="../Assets/Js/TradingPage/CouponsDetail.js"></script>

</head>
<body>


    <!--隐藏控件记录值-->
    <input type="hidden" id="hidCouponsID" value="@ViewBag.CID" />
    <input type="hidden" id="hidOctWapWebAddrDomain" value="@ViewBag.OctWapWeb_AddrDomain" />



    <!--面包屑导航-->
    <div class="nav am-cf am-nbfc" data-am-sticky="{top:0}">
        <ul class="am-breadcrumb">
            <li><a href="../NavPage/Index" class="am-icon-home">首页</a></li>
            <li><a href="../NavPage/NavSubList?NavName=优惠券管理">优惠券管理</a></li>
            <li class="am-active">优惠券详情</li>
        </ul>
    </div>

    <!--主体内容-->
    <div class="main am-nbfc">

        <!--标题栏-->
        <div class="main-title am-cf am-nbfc">
            <div class="title-txt-div am-fl">
                <i class="am-icon-list"></i> 优惠券详情
            </div>
            <div class="title-btn-div am-fl">
                <!--<button class="title-btn am-btn am-btn-xs am-btn-danger am-round" onclick="openAddWin()"><i class="am-icon-plus-circle"></i> 添加类别</button>-->
            </div>
        </div>

        <!--操作表单-->
        <div class="form-main am-nbfc am-padding">
            <dl class="order-basic">
                <dt>基本信息</dt>
                <dd>
                    <span>优惠券ID：</span><val id="CouponsID"></val>
                    <span>商家UserID：</span><val id="ShopUserID">

                    </val>
                    <span>抵用金额：</span>&#165;<val id="UseMoney">

                    </val>
                    <span>使用折扣：</span><val id="UseDiscount">

                    </val>
                </dd>
                <dd>
                    <span>总数(剩余)：</span><val id="NumTotal"></val>
                    <span>消费满可用：</span>&#165;<val id="ExpenseReachSum">

                    </val>
                    <span>发放方式：</span><val id="IssueType">

                    </val>
                    <span>消费满下单发放：</span>&#165;<val id="IssueExpenseSum">

                    </val>
                </dd>
                <dd>
                    <span>商城券：</span><val id="IsMallCoupons"></val>
                    <span>暂停发放：</span><val id="IssuePause">

                    </val>
                    <span>重复领取：</span><val id="IsRepeatGet">

                    </val>
                    <span>线下验证使用：</span><val id="IsOfflineUse">

                    </val>
                    <span>锁定 ：</span><val id="IsLock">

                    </val>
                </dd>
                <dd>
                    <span>发布时间：</span><val id="WriteDate"></val>
                    <span>使用时间范围：</span>&#165;<val id="UseTimeRange">

                    </val>
                </dd>
                <dd>
                    <span>已发放：</span>
                    <val id="CountIssue"></val>
                    <span>已使用：</span>
                    <val id="CountUsed">

                    </val>
                </dd>

                <dd>
                    <span>优惠券标题：</span><val id="CouponsTitle"></val>
                </dd>
                <dd>
                    <span>优惠券描述：</span><val id="CouponsDesc"></val>
                </dd>
            </dl>

            <dl class="order-basic">
                <dt>可使用商品</dt>
                <dd class="order-goods-list" id="UseGoodsTitleArrDD">
                    @*<div>
                            <span>
                                <a href="#" target="_blank">格菲卡纳 毛 [2XL 黑色]</a>
                            </span>
                        </div>*@
                </dd>
            </dl>

            <dl class="order-basic">
                <dt>可使用店铺</dt>
                <dd class="order-goods-list" id="UseShopNameArrDD">
                    @*<div>
                            <span>
                                <a href="#" target="_blank">店铺名称店铺名称店铺名称店铺名称(100006)</a>
                            </span>

                        </div>*@
                </dd>
            </dl>




        </div>

    </div>



</body>
</html>
