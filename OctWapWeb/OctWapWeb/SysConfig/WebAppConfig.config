<?xml version="1.0" encoding="utf-8"?>
<configuration>


	<!--项目的主要设置信息-->
	<appSettings>


		<!--API接口请求配置-->
		<add key="UserKeyID" value="8866" />
		<add key="UserKey" value="9af7f46a-ea52-4aa3-b8c3-9fd484c69852" />

		<!--全局变量，是否开启写入日志 [是/否]-->
		<add key="IsWriteSystemLog" value="否" />

