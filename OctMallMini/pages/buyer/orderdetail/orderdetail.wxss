/* pages/buyer/orderdetail/orderdetail.wxss */
/*Logo区*/
.logo-content {
  background: -webkit-linear-gradient(left, #FF8C01, #FF5300);
  /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(right, #FF8C01, #FF5300);
  /* Opera 11.1 - 12.0 */
  background: -moz-linear-gradient(right, #FF8C01, #FF5300);
  /* Firefox 3.6 - 15 */
  background: linear-gradient(to right, #FF8C01, #FF5300);
  /* 标准的语法 */
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 10px;
  color: #ffffff;
}

.logo-left {
  width: 100%;
  font-size: 35rpx;
  font-weight: bold;
}

.logo-left view {
  font-size: 26rpx;
  font-weight: normal;
}


.logo-right {
  /*background: url(../../Imgs/Icon/order_trucks.png) right center no-repeat;*/
  background-size: 50px;
  padding-left: 20px;
}

.logo-right image {
  width: 50px;
  height: 35px;
}


/*收货地址和快递信息*/
.express-recei {
  margin-top: 8px;
  background: #ffffff;
  padding: 10px;
}

.express-recei navigator {
  display: block;
}

.express-recei .express-msg {
  display: flex;
  justify-content: space-between;
  flex-wrap: nowrap;
  align-items: center;
}

.express-msg-mid {
  overflow: hidden;
  width: 100%;
  line-height: 120%;
}

.express-msg-left {
  width: 40px;
  /* background: url(../../Imgs/Icon/order_greentrucks.png) left center no-repeat; */
  background-size: 25px;
  height: 25px;
}

.express-msg-left image {
  width: 50rpx;
  height: 50rpx;
}

.express-mid-content {
  color: #339629;
  font-size: 24rpx;
}

.express-mid-date {
  color: #c4c4c4;
  padding-top: 5px;
  font-size: 24rpx;
}

.express-msg-right {
  width: 15px;
  text-align: right;
  /* background: url(../../Imgs/Icon/arrows_right.png) right center no-repeat; */
  background-size: 15px;
}

.express-msg-right image {
  width: 30rpx;
  height: 30rpx;
}

.express-msg {
  border-bottom: 1px dashed #f2f2f2;
  padding-bottom: 5px;
}

/*收货地址*/
.recei-msg {
  display: flex;
  justify-content: space-around;
  flex-wrap: nowrap;
  align-items: center;
  margin-top: 5px;
  font-size: 24rpx;
}

.recei-msg-mid {
  width: 100%;
}

.recei-msg-left {
  width: 40px;
  /* background: url(../../Imgs/Icon/order_addr.png) left center no-repeat; */
  background-size: 25px;
  height: 25px;
}

.recei-msg-left image {
  width: 50rpx;
  height: 50rpx;
}

.recei-msg-nametel {
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.recei-msg-addr {
  color: gray;
}

.recei-msg-right {
  border: 1px solid #f3f3f3;
  padding: 10px 5px;
  text-align: center;
  margin-left: 10px;
  color: gray;
  border-radius: 10px;
  font-size: 10px;
}

/*到店消费/自取  店铺地址*/
.shop-addr-nav {
  padding: 8px 0;
  padding-bottom: 0;
  margin-top: 8px;
  background: #ffffff;
  display: flex;
  align-items: start;
}

.shop-addr-nav-left {
  width: 90px;
  font-size: 28rpx;
}

.shop-addr-nav-left view {
  color: gray;
}

.shop-addr-nav-left image {
  width: 10px;
  height: 10px;
}

.shop-addr-nav-mid {
  width: 100%;
  font-size: 28rpx;
}

.shop-addr-nav-right {
  padding-left: 6px;
  font-size: 24rpx;
}

.shop-addr-nav-right image {
  width: 25px;
  height: 25px;
  margin-bottom: -10rpx;
}


/*订单商品和价格区*/
.order-list {
  margin-top: 8px;
}

.order-list .order-item {
  background: white;
  margin-bottom: 10px;
  font-size: 24rpx;
}

.order-item-top {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 10px;
  font-weight: bold;
  font-size: 26rpx;
}

.order-item-top navigator {
  display: flex;
  align-items: center;
  /* background: url(../../Imgs/Icon/arrows_right.png) right center no-repeat; */
  background-size: 12px;
  padding-right: 20px;
}

.order-item-top image {
  width: 22px;
  height: 22px;
  border-radius: 5px;
  margin-right: 5px;
}

.order-item-top .arrows-right {
  width: 25rpx;
  height: 25rpx;
  margin-left: 10rpx;
}

.order-status-txt {
  color: red;
}

/*商品列表*/
.order-item-mid {
  background: #FAFAFA;
  padding: 0 10px;
}

.order-goods-item {
  display: flex;
  justify-content: space-between;
  flex-wrap: nowrap;
  padding-bottom: 5px;
  padding-top: 5px;
  border-bottom: 1px dashed #f4f4f4;
}

.goods-item-left {
  padding-right: 10px;
}

.goods-item-left image {
  width: 55px;
  height: 55px;
  border-radius: 2px;
}

.goods-item-mid {
  width: 100%;
  padding-right: 5px;
}

.goods-item-mid span {
  display: block;
}

.goods-item-title {
  height: 85rpx;
}

.goods-item-spec {
  color: gray;
  font-size: 10px;
}

.goods-item-right {
  text-align: right;
}

.goods-item-ordernum {
  color: gray;
}

.order-item-bottom {
  text-align: right;
  background: white;
  padding: 8px 10px;
  border-bottom: 1px solid #FAFAFA
}

.order-item-bottom b {
  color: #ff0000;
}

.order-item-btn {
  padding: 8px 10px;
  background: white;
  text-align: right;
}

.order-item-btn view {
  border: 1px solid #e0e0e0;
  background: white;
  border-radius: 30px;
  padding: 4px 10px;
  font-size: 10px;
  margin-left: 5px;
}

.order-goods-sum view,
.order-sum-ul view {
  display: flex;
  justify-content: space-between;
  padding-top: 4px;
  color: gray;
}

.order-item-sum {
  background: white;
  padding-left: 8px;
  padding-right: 8px;
  padding-bottom: 10px;
}

.order-sum-ul view {
  color: black;
}

.pay-price-b {
  color: red;
}

/*发票信息*/
.invoice-msg {
  padding: 10px;
  padding-right: 0px;
  background: white;
  margin-top: 8px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 24rpx;
}

.invoice-msg-right {
  color: gray;
  /* background: url(../../Imgs/Icon/arrows_right.png) right center no-repeat; */
  background-size: 15px;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}

.invoice-msg-right image {
  width: 26rpx;
  height: 26rpx;
  margin-left: 10rpx;
}

/*客服与投诉*/
.customer-complain {
  padding: 10px;
  background: white;
  margin-top: 8px;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.customer-complain view {
  color: gray;
  text-align: center;
  display: flex;
  align-items: center;
  padding: 0 15px;
  font-size: 24rpx;
}

.customer-complain view image {
  width: 20px;
  height: 20px;
  margin-right: 5px;
}

.customer-complain .complain-item image {
  width: 50rpx;
  height: 50rpx;
}

/*订单其他信息*/
.order-other {
  padding: 10px;
  background: white;
  margin-top: 8px;
  margin-bottom: 60px;
}

.order-other-ul view {
  display: flex;
  justify-content: space-between;
  padding-bottom: 8px;
  font-size: 24rpx;
}

.order-other-val {
  color: gray;
  display: flex;
  align-items: center;
}

.order-other-val b {
  font-weight: normal;
}

.btn-copy {
  border: 1px solid #e0e0e0;
  background: no-repeat;
  font-size: 10px;
  padding: 0px 10px;
  border-radius: 10px;
  margin-left: 5px;
  cursor: pointer;
  height: 25rpx;
  padding-top: 5rpx;
}

/*页脚*/
.oct-footer {
  padding: 10px 0;
  justify-content: flex-end;
  border-top: 1px solid #f6f6f6;
  align-items: center;
}

.oct-footer view {
  border: 1px solid #e0e0e0;
  color: gray;
  border-radius: 20px;
  padding: 3px 10px;
  cursor: pointer;
  font-size: 12px;
  display: flex;
  align-items: center;
  width: 50px;
  margin-right: 6px;
}

.oct-footer button{
  padding: 0;
  border: none;
  font-size: 12px;
  font-weight: normal;
  color: #FF5100;
}


.oct-footer view.footer-btn-current {
  border: 1px solid #FF5100;
  color: #FF5100;
}

/*申请发票窗口*/
.win-invoice-item {
  padding: 8px 10px;
}

.win-invoice-item-title {
  font-weight: bold;
  padding: 5px 0;
}

.win-invoice-list {
  padding-top: 5px;
}

.win-invoice-item {
  display: flex;
  justify-content: space-between;
  border-bottom: 1px dashed #FAFAFA;
}

.win-invoice-right {
  color: gray;
}

/* 赠品窗口 */
.win-gift-list {}

.win-gift-list navigator {
  display: flex;
  justify-content: space-between;
  border-bottom: 1px dashed #FAFAFA;
  padding: 5px 10px;
}


/*赠送信息*/
.giving-msg {
  padding: 8px;
  background: #ffffff;
}

.giving-msg view {
  padding: 3px;
  color: gray;
  font-size: 12px;
}

/*到店消费/自取验证码*/
.check-code {
  background: white;
  display: flex;
  justify-items: center;
  justify-content: space-between;
  padding: 5px 10px;
}

.check-code-left {
  text-align: center;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  width: 50%;
}

.check-code-left view {
  font-size: 14px;
}

.check-code-left .check-code-number {
  font-size: 14px;
  padding-top: 0px;
  padding-bottom: 5px;
}

.check-code-number text {
  font-size: 20px;
  color: red;
}

.check-code .btn-reset {
  background: #ffffff;
  border-radius: 20px;
  border: 1px solid #c4c4c4;
  padding: 5px 20px;
  font-size: 12px;
  color: gray;
  width: 130px;
}

.check-code-right {
  width: 50%;
  text-align: center;
}

.check-code-right image {
  width: 120px;
  height: 120px;
}

.check-code-left .check-order-id {
  font-size: 12px;
  color: gray;
}

.order-price-sum{
  display: flex;
  align-items: center;
}
.order-type-label{
  background: red;
  color: white;
  padding: 2px 5px;
  border-radius: 10px;
  margin-right: 10px;
  font-size: 10px;
}