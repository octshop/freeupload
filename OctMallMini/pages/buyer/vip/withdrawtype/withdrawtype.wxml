<!--pages/buyer/vip/withdrawtype/withdrawtype.wxml-->
<view class="nav-bar-content">
	<navBar id="NavBar" class="nav-bar" title-text="选择提现方式" back-icon="/assets/imgs/icons/navbar_back.png"
		home-icon="/assets/imgs/icons/navbar_question.png" background="#F2F2F2" bindhome="home" bindback="back" />
</view>

<!-- 主体内容  -->
<view class="oct-content" style="margin-top:{{octContentMarginTop}}px">

	<view class="withdraw-list">
		<view class="withdraw-top">
			提现到：
		</view>
		<view class="withdraw-bottom">
			<navigator url="../withdrawsubmit/withdrawsubmit?ToType=WeChat" class="withdraw-type-weixin">
				<image src="{{apiWebDoamin}}/Assets/Imgs/Icon/vip_weixin.png"></image>
				<view>微信钱包</view>
			</navigator>
			<navigator url="../withdrawsubmit/withdrawsubmit?ToType=Alipay" class="withdraw-type-alipay">
				<image src="{{apiWebDoamin}}/Assets/Imgs/Icon/vip_alipay.png"></image>
				<view>支付宝</view>
			</navigator>
			<navigator url="../withdrawsubmit/withdrawsubmit?ToType=Bank" class="withdraw-type-bank">
				<image src="{{apiWebDoamin}}/Assets/Imgs/Icon/vip_bank.png"></image>
				<view>银行卡</view>
			</navigator>
		</view>
	</view>

</view>