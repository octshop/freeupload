<body>


    <!--面包屑导航-->
    <div class="nav am-cf am-nbfc" data-am-sticky="{top:0}">
        <ul class="am-breadcrumb">
            <li><a href="../NavPage/Index" class="am-icon-home">首页</a></li>
            <li><a href="../NavPage/NavSubList?NavName=抽奖管理">抽奖管理</a></li>
            <li class="am-active">中奖发货信息</li>
        </ul>
    </div>

    <!--主体内容-->
    <div class="main am-nbfc">

        <!--标题栏-->
        <div class="main-title am-cf am-nbfc">
            <div class="title-txt-div am-fl">
                <i class="am-icon-list"></i> 中奖发货信息
            </div>
            <div class="title-btn-div am-fl">
                <!--<button class="title-btn am-btn am-btn-xs am-btn-danger am-round" onclick="window.location.href='../LuckyDrawPage/LuckyDrawMsgAdd'"><i class="am-icon-plus-circle"></i> 发布抽奖</button>-->
            </div>
        </div>

        <!--搜索栏-->
        <div class="search-bar am-btn-toolbar am-cf">

            <div class="am-btn-group am-btn-group-xs am-cf item-txt-css">
                <input type="number" class="keyword-input am-form-field am-fl" id="LuckyDrawResultID_se" placeholder="中奖ID" />
            </div>

            <div class="am-btn-group am-btn-group-xs am-cf item-txt-css">
                <input type="text" class="keyword-input am-form-field am-fl" id="ExpressName_se" placeholder="快递名称" />
            </div>

            <div class="am-btn-group am-btn-group-xs am-cf item-txt-css" style="width: 200px">
                <input type="text" class="keyword-input am-form-field am-fl" id="ExpressNumber_se" placeholder="快递单号" style="width: 180px;" />
            </div>

            <div class="am-btn-group am-btn-group-xs am-cf item-txt-css" style="width: 200px">
                <input type="text" class="keyword-input am-form-field am-fl" id="SendTelNumber_se" placeholder="发货联系手机" style="width: 180px;" />
            </div>

            <div class="am-btn-group am-btn-group-xs am-cf item-txt-css" style="width: 200px">
                <input type="text" class="keyword-input am-form-field am-fl" id="SendGoodsMemo_se" placeholder="发货备注" style="width: 180px;" />
            </div>

            <div class="datetime-div am-btn-group am-btn-group-xs am-cf" style="width: 150px;">
                <div class="am-form-group am-form-icon">
                    <i class="am-icon-calendar"></i>
                    <input type="text" id="WriteDate_se" class="datetime-input am-form-field" placeholder="发货时间" data-am-datepicker="{theme: 'success',}" style="background:white;width:150px; cursor:pointer" readonly>
                </div>
            </div>


            <div class="am-btn-group am-btn-group-xs am-cf" style="width: 210px;">
                <button type="button" id="btnSearch" class="search-btn am-fl am-btn am-radius am-btn-success">搜索</button>
            </div>


        </div>

        <!--表单区域-->
        <div class="table-div">

            <table class="table-content am-table-striped am-table-hover" width="100%">
                <thead>
                    <tr class="table-head am-success">
                        <td width="15%">中奖ID</td>
                        <td width="15%">快递名称</td>
                        <td width="15%">快递单号</td>
                        <td width="15%">发货联系手机</td>
                        <td width="27%">发货备注</td>
                        <td width="13%">发货时间</td>
                    </tr>
                </thead>
                <tbody id="TbodyTrPage">
                    <tr>
                        <td><a href="..//LuckyDrawPage/LuckyDrawResult?LRID=5" target="_blank">234332</a></td>
                        <td>
                            顺风
                        </td>
                        <td>56356655855</td>
                        <td>13203156532</td>
                        <td>发货备注发货备注</td>
                        <td>2019-05-05 13:27:45 <font color="red">12</font></td>
                    </tr>
                </tbody>
            </table>

            <!--表格信息操作区与分页-->
            <div class="table-exe-div am-cf">
                <div class="table-exe-btn am-fl">
                    <div class="table-btn-bottom am-btn-group am-btn-group-xs">
                        <!--<button type="button" class="am-btn am-btn-default" onclick="delAllLuckyDrawJoinMsgArr()"><span class="am-icon-trash-o"></span> 批量删除</button>-->
                    </div>
                </div>
                <div class="page-bar am-fr">
                    <ul class="am-pagination" id="PageBar1">
                        <li><a href="#">«</a></li>
                        <li><a href="#">1</a></li>
                        <li><span>...</span></li>
                        <li><a href="#">2</a></li>
                        <li><a href="#">3</a></li>
                        <li class="am-active"><a href="#">4</a></li>
                        <li><a href="#">5</a></li>
                        <li><a href="#">6</a></li>
                        <li><span>...</span></li>
                        <li><a href="#">500</a></li>
                        <li><input type="text" class="page-go-text am-form-field" placeholder="跳转页" /></li>
                        <li><a href="#">»</a></li>
                    </ul>
                </div>
            </div>

            <!--备注说明-->
            <div class="memo-content">
                注意: <span>收货地址请与中奖买家联系确认</span>
            </div>
        </div>
    </div>



    <!---------------发货详情窗口---------------->
    <div class="detail-win" id="DetailWin" style="display:none;">
        <div class="detail-main">
            <ul class="detail-ul">
                <li>
                    <label class="am-form-label">中奖ID:</label>
                    <span id="LuckyDrawResultID_Win">00</span>
                    <label class="am-form-label">抽奖ID:</label>
                    <a id="LuckyDrawID_Win" href="#" target="_blank">2342</a>
                </li>
                <li>
                    <label class="am-form-label">奖项名称:</label>
                    <val id="AwardsItem_Win">一等奖</val>
                </li>
                <li class="nickname-li">
                    <div>
                        <label class="am-form-label"> 买家昵称:</label>
                        <val id="NickName_Win"><img src="../Assets/Imgs/header_1.jpg" />000</val>
                    </div>
                    <div>
                        <label class="am-form-label">买家联系电话:</label>
                        <val id="LinkMobile_Win">13203156008</val>
                    </div>
                </li>
                <li>
                    <label class="am-form-label">中奖时间:</label>
                    <val id="WriteDate_Win">2020-08-27 15:07:00.383</val>
