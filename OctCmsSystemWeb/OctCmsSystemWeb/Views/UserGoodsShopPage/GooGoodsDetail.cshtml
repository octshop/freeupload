
@{
    Layout = null;
}

<!DOCTYPE html>

<html style="overflow: auto; overflow-x: hidden;">
<head>
    <!--
           <meta name="viewport" content="width=device-width initial-scale="1" />
       viewport是网页默认的宽度和高度，上面这行代码的意思是，网页宽度默认等于屏幕宽度（width=device-width），
       原始缩放比例（initial-scale=1）为1.0，即网页初始大小占屏幕面积的100%。 user-scalable=no  不允许缩放网页 -->
    <meta name="viewport" content="width=800,user-scalable=no" initial-scale="1" />
    <title>商品信息详情</title>
    <link href="../Assets/Lib/AmazeUI-2.7.2/css/amazeui.min.css" rel="stylesheet" />
    <link href="../Assets/Lib/AdminFrame/AdminPagePublic.css" rel="stylesheet" />
    <link href="../Assets/Css/UserGoodsShopPage/GooGoodsDetail.css" rel="stylesheet" />

    <script src="../Assets/Lib/AmazeUI-2.7.2/js/jquery-2.2.2.min.js"></script>
    <script src="../Assets/Js/UserGoodsShopPage/GooGoodsDetail.js"></script>

</head>
<body>

    <!--隐藏控件记录值-->
    <input type="hidden" id="hidOctWapWebAddrDomain" value="@ViewBag.OctWapWebAddrDomain" />
    <input type="hidden" id="hidGoodsID" value="@ViewBag.GoodsID" />


    <!--面包屑导航-->
    <div class="nav am-cf am-nbfc" data-am-sticky="{top:0}">
        <ul class="am-breadcrumb">
            <li><a href="../NavPage/Index" class="am-icon-home">首页</a></li>
            <li><a href="../NavPage/NavSubList?NavName=商品管理">商品管理</a></li>
            <li class="am-active">商品信息详情</li>
        </ul>
    </div>

    <!--主体内容-->
    <div class="main am-nbfc">

        <!--标题栏-->
        <div class="main-title am-cf am-nbfc">
            <div class="title-txt-div am-fl">
                <i class="am-icon-list"></i> 商品信息详情
            </div>
            <div class="title-btn-div am-fl">
                <!--<button class="title-btn am-btn am-btn-xs am-btn-danger am-round" onclick="openAddWin()"><i class="am-icon-plus-circle"></i> 添加类别</button>-->
            </div>
        </div>

        <!--操作表单-->
        <div class="form-main am-nbfc am-padding">
            <div class="order-status">
                <div class="order-status-title">
                    <val id="StatusTtitle"></val>
                </div>
                <div class="order-status-desc">
                    <val id="StatusDesc"></val>
                </div>
                <div class="order-status-dynamic">
                    <val id="GoodsCheckReason"></val>
                </div>
            </div>
            <dl class="order-basic">
                <dt>基本信息</dt>
                <dd>
                    <span>商品ID：</span><val id="GoodsID"></val>
                    <span>商品类目：</span><val id="GoodsTypeID">
                      
                    </val>
                    <span>锁定：</span><val id="IsLock">
                       
                    </val>
                    <span>发布时间：</span><val id="WriteDate">
                      
                    </val>
                    <span><a id="GoodsDetailPreA" href="#" target="_blank" style="font-size:16px; font-weight:bold;">查看商品预览</a></span>
                </dd>
                <dd>
                    <span>商品标题：</span><val id="GoodsTitle"></val>
                </dd>
                <dd>
                    <span>价格：</span><val id="GoodsPrice">
                      
                    </val>
                    <span>市场价 ：</span><val id="MarketPrice">
                      
                    </val>
                    <span>折扣 ：</span><val id="Discount">
                      
                    </val>
                    <span>销量 ：</span><val id="SaleCount">
                      
                    </val>
                    <span>已支付：</span><val id="PaidCount">
                      
                    </val>
                </dd>
                <dd>
                    <span>规格属性：</span><val id="IsSpecParam">
                       
                    </val>
                    <span>下架：</span><val id="IsUnSale">
                       
                    </val>
                    <span>货到付款：</span><val id="IsPayDelivery">
                       
                    </val>
                    <span>到店消费/自取：</span><val id="IsShopExpense">
                      
                    </val>
                    <span>到店付：</span><val id="IsOfflinePay">
                       
                    </val>
                </dd>
                <dd>
                    <span>商家推荐：</span><val id="IsShopCommend"></val>
                    <span>运费模板ID：</span><val id="FtID"></val>
                    <span>店铺商品类别ID：</span><val id="ShopGoodsTypeID"></val>
                </dd>
                <dd>
                    <span>商品必填属性：</span><val id="GoodsTypeNeedProp">
                     
                    </val>
                </dd>
                <dd>
                    <span>商品自定义属性：</span><val id="GoodsTypeCustomProp"></val>
                </dd>
                <dd>
                    <span>包装售后说明：</span><val id="PackAfterSaleDesc"></val>
                </dd>
                <dd>
                    <span>商品备注：</span><val id="GoodsMemo"></val>
                </dd>
            </dl>

            <dl class="order-basic">
                <dt>赠品信息</dt>
                <dd class="order-goods-list" id="GoodsGiftList">
                    @*<div>
                            <span>
                                <a href="#" target="_blank">格菲卡纳 毛呢连衣裙冬季女装新款韩版时尚套装女休闲秋天大码衣服女士裙子矮个子小香风两件套装裙子 [2XL 黑色]</a>
                            </span>
                            <span>
                                &#165;456.00  x 2
                            </span>
                        </div>*@
                </dd>
            </dl>


            <dl class="order-basic buyer-msg">
                <dt>店铺信息</dt>
                <dd>
                    <span>店铺ID：</span><val id="ShopID"><a href="#"></a></val>
                    <span>店铺类目：</span><val id="ShopTypeID"></val>
                    <span>公司ID：</span><val id="CompanyID"></val>
                </dd>
                <dd>
                    <span>商家UserID：</span><val id="ShopUserID"></val>
                    <span>地址：</span><val id="DetailAddr"></val>
                </dd>
                <dd>
                    <span>联系人：</span><val id="LinkMan">小黄人</val>
                    <span>联系人手机号：</span><val id="LinkManMobile"><a href="tel:"></a></val>
                    <span>店铺手机号：</span><val id="ShopMobile"><a href="tel:"></a></val>
                    <span>发货联系电话：</span><val id="SendMobile"><a href="tel:"></a></val>
                </dd>
            </dl>



        </div>
    </div>



</body>
</html>
