
@{
    Layout = null;
}

<!DOCTYPE html>

<html style="overflow: auto; overflow-x: hidden;">
<head>
    <!--
           <meta name="viewport" content="width=device-width initial-scale="1" />
       viewport是网页默认的宽度和高度，上面这行代码的意思是，网页宽度默认等于屏幕宽度（width=device-width），
       原始缩放比例（initial-scale=1）为1.0，即网页初始大小占屏幕面积的100%。 user-scalable=no  不允许缩放网页 -->
    <meta name="viewport" content="width=800,user-scalable=no" initial-scale="1" />
    <title>转账银行信息</title>
    <link href="../Assets/Lib/AmazeUI-2.7.2/css/amazeui.min.css" rel="stylesheet" />
    <link href="../Assets/Lib/AdminFrame/AdminPagePublic.css" rel="stylesheet" />

    <script src="../Assets/Lib/AmazeUI-2.7.2/js/jquery-2.2.2.min.js"></script>
    <script src="../Assets/Lib/AmazeUI-2.7.2/js/amazeui.min.js"></script>
    <script src="../Assets/Lib/AdminFrame/AdminPagePublic.js"></script>
    <script src="../Assets/Lib/PublicTool.js"></script>
    <script src="../Assets/Lib/AlertConfirmWin/AlertConfirmWin.js"></script>
    <script src="../Assets/Lib/DialogWinDrag/DialogWinDrag.js"></script>
    <script src="../Assets/Js/TradingPage/PayTransBankMsg.js"></script>

</head>
<body>


    <!--面包屑导航-->
    <div class="nav am-cf am-nbfc" data-am-sticky="{top:0}">
        <ul class="am-breadcrumb">
            <li><a href="../NavPage/Index" class="am-icon-home">首页</a></li>
            <li><a href="../NavPage/NavSubList?NavName=支付系统">支付系统</a></li>
            <li class="am-active">转账银行信息</li>
        </ul>
    </div>

    <!--主体内容-->
    <div class="main am-nbfc">

        <!--标题栏-->
        <div class="main-title am-cf am-nbfc">
            <div class="title-txt-div am-fl">
                <i class="am-icon-list"></i> 转账银行信息
            </div>
            <div class="title-btn-div am-fl">
                <button class="title-btn am-btn am-btn-xs am-btn-danger am-round am-margin-left-xs" onclick="openAddWin()"><i class="am-icon-plus-circle"></i> 添加</button>
            </div>
        </div>

        <!--搜索栏-->
        <div class="search-bar am-btn-toolbar am-cf">

            <div class="am-btn-group am-btn-group-xs am-cf item-txt-css">
                <input type="number" class="keyword-input am-form-field am-fl" id="TransBankID_se" placeholder="银行信息ID" />
            </div>
            <div class="am-btn-group am-btn-group-xs am-cf item-txt-css">
                <input type="number" class="keyword-input am-form-field am-fl" id="ShopUserID_se" placeholder="商家UserID" />
            </div>
            <div class="am-btn-group am-btn-group-xs am-cf item-txt-css">
                <input type="text" class="keyword-input am-form-field am-fl" id="BankName_se" placeholder="银行名称" />
            </div>
            <div class="am-btn-group am-btn-group-xs am-cf item-txt-css">
                <input type="text" class="keyword-input am-form-field am-fl" id="BankAccName_se" placeholder="银行户名" />
            </div>
            <div class="am-btn-group am-btn-group-xs am-cf item-txt-css">
                <input type="text" class="keyword-input am-form-field am-fl" id="OpenAccBank_se" placeholder="开户行" />
            </div>
            <div class="am-btn-group am-btn-group-xs am-cf item-txt-css">
                <input type="text" class="keyword-input am-form-field am-fl" id="BankAccount_se" placeholder="银行账号" />
            </div>
            <div class="am-btn-group am-btn-group-xs am-cf" style="width: 150px;">
                <select id="TransType_se" data-am-selected="{btnWidth:'130',btnSize: 'xs',btnStyle: 'default'}">
                    <option value=" " selected>信息类别</option>
                    <option value="Platform">平台</option>
                    <option value="Shop">商家</option>
                </select>
            </div>
            <div class="am-btn-group am-btn-group-xs am-cf" style="width: 150px;">
                <select id="IsLock_se" data-am-selected="{btnWidth:'130',btnSize: 'xs',btnStyle: 'default'}">
                    <option value=" " selected>锁定</option>
                    <option value="true">是</option>
                    <option value="false">否</option>
                </select>
            </div>

            <div class="datetime-div am-btn-group am-btn-group-xs am-cf" style="width: 150px;">
                <div class="am-form-group am-form-icon">
                    <i class="am-icon-calendar"></i>
                    <input type="text" id="WriteDate_se" style="background:none; width: 130px;" class="datetime-input am-form-field" placeholder="写入时间" data-am-datepicker="{theme: 'success',}" readonly>
                </div>
            </div>

            <div class="am-btn-group am-btn-group-xs am-cf" style="width: 210px;">
                <button type="button" id="btnSearch" class="search-btn am-fl am-btn am-radius am-btn-success">搜索</button>
            </div>


        </div>

        <!--表单区域-->
        <div class="table-div">

            <table class="table-content am-table-striped am-table-hover" width="100%">
                <thead>
                    <tr class="table-head am-success">
                        <td width="4%"><input type="checkbox" id="SelAllCb" name="SelAllCb" onchange="toggleSelAll()" /></td>
                        <td width="10%">银行信息ID</td>
                        <td width="10%">商家UserID<i style="color: #939393;display:block">店铺名称</i></td>
                        <td width="13%">银行名称</td>
                        <td width="15%">开户行</td>
                        <td width="16%">银行账号</td>
                        <td width="10%">信息类别</td>
                        <td width="6%">锁定</td>
                        <td width="10%">写入时间</td>
                        <td width="6%">
                            操作
                        </td>
                    </tr>
                </thead>
                <tbody id="TbodyTrPage">
                    <tr>
                        <td><input type="checkbox" /></td>
                        <td>234324</td>
                        <td>235433<i style="color: #939393;display: block"><a href="#" target="_blank">店铺名称</a></i></td>
                        <td>中国工商银行</td>
                        <td>中国工商银行长沙支行</td>
                        <td>62565465489874654685465</td>
                        <td>商家(Shop)</td>
                        <td>是</td>
                        <td>
                            2019-05-05 13:27:45
                        </td>
                        <td>
                            <button class="table-btn am-btn am-btn-default am-btn-xs am-text-secondary am-round">锁定</button>
                            <button class="table-btn am-btn am-btn-default am-btn-xs am-text-success am-round">修改</button>
                        </td>
                    </tr>
                </tbody>
            </table>

            <!--表格信息操作区与分页-->
            <div class="table-exe-div am-cf">
                <div class="table-exe-btn am-fl">
                    <div class="table-btn-bottom am-btn-group am-btn-group-xs">
                        <button type="button" class="am-btn am-btn-default" onclick="delPayTransBankMsgArr()"><span class="am-icon-trash-o"></span> 批量删除</button>
                    </div>
                </div>
                <div class="page-bar am-fr">
                    <ul class="am-pagination" id="PageBar1">
                        <li><a href="#">«</a></li>
                        <li><a href="#">1</a></li>
                        <li><span>...</span></li>
                        <li><a href="#">2</a></li>
                        <li><a href="#">3</a></li>
                        <li class="am-active"><a href="#">4</a></li>
                        <li><a href="#">5</a></li>
                        <li><a href="#">6</a></li>
                        <li><span>...</span></li>
                        <li><a href="#">500</a></li>
                        <li><input type="text" class="page-go-text am-form-field" placeholder="跳转页" /></li>
                        <li><a href="#">»</a></li>
                    </ul>
                </div>
            </div>

            <!--备注说明-->
            <div class="memo-content">
                转账银行信息
            </div>


        </div>
    </div>





    <!--添加与编辑窗口主体内容-->
    <div class="add-edit-win" id="AddEditWin" style="display:none;">
        <div class="add-edit-main">
            <ul class="add-edit-ul">
                <li>
                    <span>信息类别：</span>
                    <select id="TransTypeWin" style="padding: 8px;">
                        <option value="Platform">平台</option>
                        <option value="Shop">商家</option>
                    </select>
                </li>
                <li>
                    <span>商家UserID：</span>
                    <input type="number" id="ShopUserIDWin" class="txt-css" placeholder="" />
                </li>
                <li>
                    <span>银行名称：</span>
                    <input type="text" id="BankNameWin" class="txt-css" placeholder="" />
                </li>
                <li>
                    <span>银行户名：</span>
                    <input type="text" id="BankAccNameWin" class="txt-css" placeholder="" />
                </li>
                <li>
                    <span>开户行：</span>
                    <input type="text" id="OpenAccBankWin" class="txt-css" placeholder="" />
                </li>
                <li>
                    <span>银行账号：</span>
                    <input type="text" id="BankAccountWin" class="txt-css" placeholder="" />
                </li>
            </ul>
        </div>
    </div>
    <!--窗口样式-->
    <style type="text/css">
        .add-edit-ul li input {
            width: 80%;
        }
    </style>


</body>
</html>
