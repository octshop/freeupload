
@{
    Layout = null;
}

<!DOCTYPE html>

<html style="overflow: auto; overflow-x: hidden;">
<head>
    <!--
         <meta name="viewport" content="width=device-width initial-scale="1" />
     viewport是网页默认的宽度和高度，上面这行代码的意思是，网页宽度默认等于屏幕宽度（width=device-width），
     原始缩放比例（initial-scale=1）为1.0，即网页初始大小占屏幕面积的100%。 user-scalable=no  不允许缩放网页 -->
    <meta name="viewport" content="width=800,user-scalable=no" initial-scale="1" />
    <title>礼品订单详情</title>

    <link href="../Assets/Lib/AmazeUI-2.7.2/css/amazeui.min.css" rel="stylesheet" />
    <link href="../Assets/Lib/AdminFrame/AdminPagePublic.css" rel="stylesheet" />
    <link href="../Assets/Css/TradingPage/OrderDetail.css" rel="stylesheet" />

    <script src="../Assets/Lib/AmazeUI-2.7.2/js/jquery-2.2.2.min.js"></script>
    <script src="../Assets/Lib/AdminFrame/AdminPagePublic.js"></script>
    <script src="../Assets/Lib/PublicTool.js"></script>
    <script src="../Assets/Js/TradingPage/PresentOrderDetail.js"></script>


</head>
<body>

    <!--隐藏控件记录值-->
    <input type="hidden" id="hidPOID" value="@ViewBag.POID" />
    <input type="hidden" id="hidOctWapWebAddrDomain" value="@ViewBag.OctWapWeb_AddrDomain" />


    <!--面包屑导航-->
    <div class="nav am-cf am-nbfc" data-am-sticky="{top:0}">
        <ul class="am-breadcrumb">
            <li><a href="../NavPage/Index" class="am-icon-home">首页</a></li>
            <li><a href="../NavPage/NavSubList?NavName=礼品管理">礼品管理</a></li>
            <li class="am-active">礼品订单详情</li>
        </ul>
    </div>

    <!--主体内容-->
    <div class="main am-nbfc">

        <!--标题栏-->
        <div class="main-title am-cf am-nbfc">
            <div class="title-txt-div am-fl">
                <i class="am-icon-list"></i> 礼品订单详情
            </div>
            <div class="title-btn-div am-fl">
                <!--<button class="title-btn am-btn am-btn-xs am-btn-danger am-round" onclick="openAddWin()"><i class="am-icon-plus-circle"></i> 添加类别</button>-->
            </div>
        </div>

        <!--操作表单-->
        <div class="form-main am-nbfc am-padding">
            <div class="order-status order-status-present">
                <div class="order-status-title">
                    <val id="StatusTtitle">到店消费/自取</val>
                </div>
                <div class="order-status-desc">
                    <val id="StatusDesc">等待买家到店消费或自取</val>
                </div>
            </div>
            <dl class="order-basic">
                <dt>基本信息</dt>
                <dd>
                    <span>礼品订单ID：</span><val id="PstOrderID">23244322</val>
                    <span>交易号：</span><val id="BillNumber">
                        2020042313331627527568231
                    </val>
                    <span>订单总额：</span>&#165;<val id="OrderPrice">
                        2562.00
                    </val>
                    <span>下单时间：</span><val id="OrderTime">2020-05-15 23:20:12</val>
                </dd>
                <dd class="order-goods-list" id="OrderGoodsListDD">
                    <div id="PresentOrderGoodsListDD">
                        <span>
                            <a href="#" target="_blank">格菲卡纳 毛呢连衣裙冬季女装新款韩版时尚套装女休闲秋天大码衣服女士裙子矮个子小香风两件套装裙子  [2XL 黑色]</a>
                        </span>
                        <span>
                            &#165;456.00  x 1
                        </span>
                    </div>
                </dd>
                <dd>
                    <span>配送方式：</span><val id="ExpressType">shop[到店消费/自取]）</val>
                    <span>
                        是否支付：
                    </span>
                    <val id="IsPaySuccess">
                        true[已支付]
                    </val>
                    <span>支付方式：</span><val id="PayWay">
                        Integral [积分支付]
                    </val>
                    <span>支付时间：</span><val id="PayTime">2020-05-15 23:20:12</val>
                </dd>
                <dd>
                    <span>买家UserID：</span><val id="BuyerUserID">10006</val>
                    <span>是否包邮：</span><val id="IsPinkage">false[不包邮-买家付运费]</val>
                    <span>是否退款：</span><val id="IsRefund"></val>
                    <span>签收完成时间：</span><val id="FinishTime">
                        2020-05-15 23:20:12
                    </val>
                </dd>

                <dd>
                    <span>订单备注：</span><val id="OrderMemo">订单备注订单备注订单备注订单备注订单备注</val>
                </dd>
            </dl>

            <dl class="order-basic">
                <dt>收货人信息</dt>
                <dd>
                    <span>收货人姓名：</span><val id="DeliName"></val>
                    <span>收货人手机号：</span><val id="Mobile"></val>
                </dd>
                <dd>
                    <span>省市县：</span><val id="RegionNameArr"></val>
                    <span>详细地址：</span><val id="DetailAddr"></val>
                </dd>
            </dl>


            <dl class="order-basic">
                <dt>发货信息</dt>
                <dd>
                    <span>送货方式：</span><val id="SendType"></val>
                    <span>快递名称：</span><val id="ExpressName">
                    </val>
                    <span>快递单号：</span><val id="ExpressNumber">
                    </val>
                    <span>送货人姓名：</span><val id="SendShopMan"></val>
                </dd>
                <dd>
                    <span>发货或送货人电话：</span><val id="SendTelNumber">
                    </val>
                    <span>发货备注：</span><val id="SendGoodsMemo"></val>
                </dd>
            </dl>

            <dl class="order-basic">
                <dt>店铺信息</dt>
                <dd>
                    <span>店铺ID：</span><val id="ShopID"></val>
                    <span>店铺名称：</span><val id="ShopName">@*<a href="#" target="_blank">开源测试君(10004)</a>*@</val>
                    <span>店铺电话：</span><val id="ShopMobile">

                    </val>
                </dd>

            </dl>




        </div>

    </div>



</body>
</html>
