<?xml version="1.0" encoding="utf-8"?>
<configuration>


	<!--项目的主要设置信息-->
	<appSettings>




		<!--============系统业务逻辑配置参数===========-->

		<!--此产品的唯一标识,每次给新客户配置服务器时，需要重新赋值-->
		<add key="ThisProductGuid" value="f4609e95-6b06-4190-ba09-c6a0e174c3b0" />

		<!--API接口请求配置-->
		<add key="UserKeyID" value="8866" />
		<add key="UserKey" value="9af7f46a-ea52-4aa3-b8c3-9fd484c69852" />

		<!--商城名称-->
		<add key="MallName" value="OcthShop免费开源大型专业级商城系统"/>

		<!--默认用户选择的城市区域代号,湖南省_长沙市-->
		<add key="SelCityRegionCodeArrDefault" value="430000_430100" />
		

		<!--限定同样产品下单时间间隔 以“秒” 为单位-->
		<add key="OftenPlaceOrderLimit" value="40" />

		<!-- //OctTradingSystemWeb (交易系统 订单与财务)  API请求地址域名  http://192.168.3.10:1500-->
		<add key="OctTradingSystemWeb_ApiDomain" value="http://tra.opencodetiger.com" />

		<!-- OctShopSystemWeb (商家后台 商家中心) Web网站地址域名  http://192.168.3.10:1700-->
		<add key="OctShopSystemWeb_ApiDomain" value="http://shop.opencodetiger.com" />

		<!-- OctCmsSystemWeb (商城平台后台管理) Web网站地址域名  http://192.168.3.10:1300-->
		<add key="OctCmsSystemWeb_ApiDomain" value="http://cms.opencodetiger.com" />

		<!-- //OctCommonCodeSystemWeb 各项目通用代码项目 API请求地址域名  http://192.168.3.10:1100-->
		<add key="OctCommonCodeSystemWeb_ApiDomain" value="http://cc.opencodetiger.com" />

		<!-- OctUserGoodsShopSystemWeb (会员店铺产品系统) API请求地址域名  http://192.168.3.10:1100-->
		<add key="OctUserGoodsShopSystemWeb_ApiDomain" value="http://ugs.opencodetiger.com" />

		<!-- 【上传文件系统服务器 域名列表 用^隔开】 OctFileUploadSystemWeb (文件上传系统) -->
		<add key="OctFileUploadSystemWebDomainListArr" value="http://fu.opencodetiger.com^http://fu.opencodetiger.com" />

		<!-- OctThirdApiCallSystemWeb (第三方系统调用) API请求地址域名 http://192.168.1.220:1600-->
		<add key="OctThirdApiCallSystemWeb_ApiDomain" value="http://tac.opencodetiger.com" />


	</appSettings>

	<!--项目的数据库连接字符串-->
	<connectionStrings>

		<!--OctCommonCodeDB (通用数据库) 各项目配置参数 Api请求Key和密钥信息 商城各项需要写入数据库表的配置信息 -->
		<add name="OctCommonCodeDB_ConnectionStrings" connectionString="Data Source=.,1772;database=OctCommonCodeDB;UID=sa;Password=OpenCodeTiger168_ItHuang;Persist Security Info=True;Max Pool Size=5000;Min Pool Size=1;Connect Timeout=30;Connection Lifetime=30;Asynchronous Processing=true"
		  providerName="System.Data.SqlClient" />

		<!-- OctTradingSystemDB (交易系统 订单与财务 数据库)  -->
		<add name="OctTradingSystemDB_ConnectionStrings" connectionString="Data Source=.,1772;database=OctTradingSystemDB;UID=sa;Password=OpenCodeTiger168_ItHuang;Persist Security Info=True;Max Pool Size=5000;Min Pool Size=1;Connect Timeout=30;Connection Lifetime=30;Asynchronous Processing=true"
		  providerName="System.Data.SqlClient" />

		<!-- OctUserGoodsShopSystemDB （会员店铺产品系统数据库) -->
		<add name="OctUserGoodsShopSystemDB_ConnectionStrings" connectionString="Data Source=.,1772;database=OctUserGoodsShopSystemDB;UID=sa;Password=OpenCodeTiger168_ItHuang;Persist Security Info=True;Max Pool Size=5000;Min Pool Size=1;Connect Timeout=30;Connection Lifetime=30;Asynchronous Processing=true"
		 providerName="System.Data.SqlClient" />

		<!-- OctAfterSaleAccCusDB （售后,投诉,客服反馈,消息系统 ) -->
		<add name="OctAfterSaleAccCusDB_ConnectionStrings" connectionString="Data Source=.,1772;database=OctAfterSaleAccCusDB;UID=sa;Password=OpenCodeTiger168_ItHuang;Persist Security Info=True;Max Pool Size=5000;Min Pool Size=1;Connect Timeout=30;Connection Lifetime=30;Asynchronous Processing=true"
		 providerName="System.Data.SqlClient" />

		<!-- OctFileUploadSystemDB （文件上传系统数据库) -->
		<add name="OctFileUploadSystemDB_ConnectionStrings" connectionString="Data Source=.,1772;database=OctFileUploadSystemDB;UID=sa;Password=OpenCodeTiger168_ItHuang;Persist Security Info=True;Max Pool Size=5000;Min Pool Size=1;Connect Timeout=30;Connection Lifetime=30;Asynchronous Processing=true"
		 providerName="System.Data.SqlClient" />



	</connectionStrings>



	<system.web>

	</system.web>
</configuration>
