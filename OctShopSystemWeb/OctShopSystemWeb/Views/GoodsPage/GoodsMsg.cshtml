                        <td>是</td>
                        <td>是</td>
                        <td>是</td>
                        <td>2019-05-05 13:27:45.563</td>
                        <td>
                            <button class="table-btn am-btn am-btn-default am-btn-xs am-text-secondary am-round"><span class="am-icon-pencil-square-o"></span></button>
                            <button class="table-btn am-btn am-btn-default am-btn-xs am-text-success  am-round"><span class="am-icon-cny"></span></button>
                            <button class="table-btn am-btn am-btn-default am-btn-xs am-text-warning  am-round"><span class="am-icon-arrow-circle-down"></span></button>

                        </td>
                    </tr>

                </tbody>
            </table>

            <!--表格信息操作区与分页-->
            <div class="table-exe-div am-cf">
                <div class="table-exe-btn am-fl">
                    <div class="table-btn-bottom am-btn-group am-btn-group-xs">
                        <button type="button" class="am-btn am-btn-default" onclick="delMsgArr()"><span class="am-icon-trash-o"></span> 删除</button>

                        <button type="button" class="am-btn am-btn-default" onclick="tglGoodsShopCommendArr('true')"><span class="am-icon-star"></span> 推荐商品</button>

                        <button type="button" class="am-btn am-btn-default" onclick="tglGoodsShopCommendArr('false')"><span class="am-icon-star-o"></span> 取消推荐</button>


                    </div>
                </div>
                <div class="page-bar am-fr">
                    <ul class="am-pagination" id="PageBar1">
                        <li><a href="#">«</a></li>
                        <li><a href="#">1</a></li>
                        <li><span>...</span></li>
                        <li><a href="#">2</a></li>
                        <li><a href="#">3</a></li>
                        <li class="am-active"><a href="#">4</a></li>
                        <li><a href="#">5</a></li>
                        <li><a href="#">6</a></li>
                        <li><span>...</span></li>
                        <li><a href="#">500</a></li>
                        <li><input type="text" class="page-go-text am-form-field" placeholder="跳转页" /></li>
                        <li><a href="#">»</a></li>
                    </ul>
                </div>
            </div>

            <!--备注说明-->
            <div class="memo-content">
                注意: <span>备注与说明</span>
            </div>
        </div>

    </div>





    <!--==========添加规格，价格，库存窗口============-->
    <div class="goods-pricespec-win" id="GoodsPriceSpecWin" style="display:none;">
        <div class="goods-pricespec-content">
            <div class="goods-pricespec-form" id="GoodsPriceSpecForm">

                <div class="goods-pricespec-title">
                    <span>规格标题</span>
